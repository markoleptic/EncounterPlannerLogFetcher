{
	"cSpell.words": ["begincast", "dotenv", "venv"],
	"cSpell.ignorePaths": [
		"events",
		"fights",
		"reports",
		".git",
		".gitignore",
		".vscode",
		"token.json",
		"requirements.txt"
	],
	"search.useIgnoreFiles": false,
	"search.exclude": {
		"**/.venv/*": true,
		"**/.vscode/*": true,
		"**/events/*": false,
		"**/reports/*": false,
		"**/fights/*": false,
		"**/.git/*": true,
		"**/.gitignore/*": true,
		"**/__pycache__/*": true
	},
	"python.analysis.typeCheckingMode": "basic",
	"black-formatter.args": ["--line-length", "120"]
}
