{
	"cSpell.words": [
		"applybuff",
		"Araz",
		"begincast",
		"cumcount",
		"dotenv",
		"ecolor",
		"elinewidth",
		"errorbar",
		"floatfmt",
		"Forgeweaver",
		"hylbrande",
		"Manaforge",
		"myzas",
		"ndarray",
		"removebuff",
		"Salhadaar",
		"Shadowguard",
		"showindex",
		"soazmi",
		"soleah",
		"tablefmt",
		"venv",
		"Voidwing",
		"xerr",
		"xlabel",
		"ylabel"
	],
	"cSpell.ignorePaths": [
		"events",
		"fights",
		"reports",
		".git",
		".gitignore",
		".vscode",
		"token.json",
		"requirements.txt"
	],
	"search.useIgnoreFiles": false,
	"search.exclude": {
		"**/.venv/*": true,
		"**/.vscode/*": true,
		"**/events/*": false,
		"**/reports/*": false,
		"**/fights/*": false,
		"**/.git/*": true,
		"**/.gitignore/*": true,
		"**/__pycache__/*": true
	},
	"python.analysis.typeCheckingMode": "basic",
	"black-formatter.args": ["--line-length", "120"]
}
